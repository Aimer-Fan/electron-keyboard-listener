{
  "name": "electron-keyboard-listener",
  "productName": "electron-keyboard-listener",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "AimerFan",
    "email": "1261105760@qq.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_keyboard_listener"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ],
        [
          "@timfish/forge-externals-plugin",
          {
            "externals": [
              "node-global-key-listener"
            ],
            "includeDeps": true
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.66",
    "@electron-forge/maker-deb": "^6.0.0-beta.66",
    "@electron-forge/maker-rpm": "^6.0.0-beta.66",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.66",
    "@electron-forge/maker-zip": "^6.0.0-beta.66",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.66",
    "@electron-forge/plugin-webpack": "6.0.0-beta.66",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "^6.0.0",
    "electron": "21.0.1",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-vue": "^9.5.1",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "node-global-key-listener": "^0.1.1",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.17",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "style-loader": "^3.0.0",
    "tailwindcss": "^3.1.8",
    "ts-loader": "^9.2.2",
    "typescript": "~4.5.4",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.7.10",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.9",
    "@timfish/forge-externals-plugin": "^0.2.0",
    "echarts": "^5.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "element-plus": "^2.2.17",
    "mitt": "^3.0.0",
    "node-global-key-listener": "^0.1.1",
    "vue": "^3.2.40"
  }
}
